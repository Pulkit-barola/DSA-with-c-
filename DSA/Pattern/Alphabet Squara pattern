// alphabet squara pattern
#include<iostream>
using namespace std;

int main(){
    int m;
    cout<<"Enter any number :- ";
    cin>>m;
    
    for (int i = 1; i <=m; i++)
    {
        char ch = 'A';
        for (int j = 1; j <=m; j++)
        {
            cout<<ch;
            ch++;
        }
        cout<<endl;
    }
    

}
